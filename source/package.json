{
	"name": "@@name@@",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"typecheck": "tsc --noEmit",
		"lint": "npx eslint src/**/*.ts",
		"lint:fix": "npx eslint --fix src/**/*.ts",
		"dev": "npx wrangler dev --config ./dist/wrangler.toml",
		"build": "ts-node-esm ./build.ts & tsc --emitDeclarationOnly --declaration & tsc-alias",
		"preview": "aerofoil port-runner \"npx wrangler dev --config ./dist/wrangler.toml --port {port}\"",
		"start": "aerofoil no-op",
		"deploy:production": "aerofoil deploy --local-package"
	},
	"type": "module",
	"types": "./dist/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.ts"
		}
	},
	"devDependencies": {
		"@aerofoil/aerofoil-core": "^0.7.0",
		"@aerofoil/af-ext-cloudflare": "^0.7.0",
		"@cloudflare/workers-types": "^4.20230321.0",
		"eslint": "^7.23.0",
		"eslint-config-aerofoil": "workspace:*",
		"ts-node": "^10.9.1",
		"tsc-alias": "^1.8.6",
		"tsconfig": "workspace:*",
		"type-fest": "^3.12.0",
		"typescript": "^5.0.2",
		"wrangler": "^3.1.0",
		"zod": "^3.21.4"
	},
	"dependencies": {
		"hono": "^3.2.4",
		"nanoid": "^4.0.2"
	}
}